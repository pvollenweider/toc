<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahia.modules.TocFilter">
        <property name="applyOnTemplateTypes" value="html"/>
        <property name="applyOnNodeTypes" value="jmix:editorialContent,jnt:contentList"/>
        <property name="skipOnConfigurations" value="include,page"/>
        <property name="priority" value="80"/>
    </bean>
</beans>